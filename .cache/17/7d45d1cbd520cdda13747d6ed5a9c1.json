":18}},{"generated":{"line":99,"column":45},"source":"src/models/knight.js","original":{"line":43,"column":0}}],"sources":{"src/models/knight.js":"import { ChessPieces } from \"./chess-pieces.js\";\n\nclass Knight extends ChessPieces {\n  constructor(hex, xPos, yPos) {\n    super(hex, xPos, yPos);\n    this.validMoves = Knight.findMoves(this.xPos, this.yPos);\n  }\n\n  checkAndUpdate(x2, y2, matrix) {\n    console.log(\"checking for valid move\");\n    if (this.checkAndMove(x2, y2, matrix)) {\n      this.validMoves = Knight.findMoves(this.xPos, this.yPos);\n      return true;\n    }\n    return false;\n  }\n\n  //find all possible moves for the knight\n  static findMoves(x, y) {\n    console.log(\"finding valid knight moves\");\n\n    let positions = {};\n    if (x + 1 <= 8) {\n      positions[x + 1] = [y + 2, y - 2].filter(val => val > 0 && val <= 8);\n    }\n    if (x - 1 > 0) {\n      positions[x - 1] = [y + 2, y - 2].filter(val => val > 0 && val <= 8);\n    }\n    if (x + 2 <= 8) {\n      positions[x + 2] = [y + 1, y - 1].filter(val => val > 0 && val <= 8);\n    }\n    if (x - 2 > 0) {\n      positions[x - 2] = [y + 1, y - 1].filter(val => val > 0 && val <= 8);\n    }\n    return positions;\n  }\n}\n\nlet lightKnight1 = new Knight(\"&#9816;\", 2, 1);\nlet lightKnight2 = new Knight(\"&#9816;\", 7, 1);\n\nlet darkKnight1 = new Knight(\"&#9822;\", 2, 8);\nlet darkKnight2 = new Knight(\"&#9822;\", 7, 8);\n\nexport { lightKnight1, lightKnight2, darkKnight1, darkKnight2 };\n"},"lineCount":null}},"error":null,"hash":"fc6c46a602d758bd31f0841dae44367f","cacheData":{"env":{}}}